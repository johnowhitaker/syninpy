{
  "name": "syninpy",
  "displayName": "syninpy",
  "description": "syntax highlighting for snippets within python files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [{
      "id": "python",
      "aliases": ["Python", "py"],
      "extensions": [".py"]
    }]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/node": "^14.x.x",
    "typescript": "^4.x.x"
  }
}